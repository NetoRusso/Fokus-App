{
  "expo": {
    "name": "fokus",
    "slug": "fokus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fokus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.netorusso.fokus",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#021123",
        "foregroundImage": "./assets/images/icon_app.png",
        "backgroundImage": "./assets/images/icon_app.png",
        "monochromeImage": "./assets/images/icon_app.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon_app.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#021123",
          "dark": {
            "backgroundColor": "#021123"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a55911dd-fb27-48ff-abcd-38daef45b6fa"
      }
    }
  }
}
